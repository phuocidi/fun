{
  "python.linting.enabled": false,
  "files.associations": {
    "__config": "cpp",
    "__nullptr": "cpp",
    "cstddef": "cpp",
    "exception": "cpp",
    "initializer_list": "cpp",
    "new": "cpp",
    "stdexcept": "cpp",
    "type_traits": "cpp",
    "typeinfo": "cpp",
    "algorithm": "cpp",
    "__locale": "cpp",
    "string": "cpp",
    "locale": "cpp",
    "clocale": "cpp",
    "iosfwd": "cpp",
    "utility": "cpp"
  },
  "clang.cxxflags": [
    "-std=c++11",
    "-std=c++14",
    "-std=c++17"
  ],
  "clang.cflags": ["c11"]
}